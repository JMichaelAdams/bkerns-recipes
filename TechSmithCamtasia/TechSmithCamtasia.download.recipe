<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest TechSmith Camtasia disk image.</string>
	<key>Identifier</key>
	<string>edu.purdue.ag.download.TechSmithCamtasia</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>TechSmithCamtasia</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.2.0</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>URLTextSearcher</string>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>http://www.techsmith.com/download/alternate.asp</string>
				<key>re_pattern</key>
				<string>(?:&lt;h4&gt;&lt;a href=&quot;)(http://download\.techsmith\.com/camtasiamac/enu/.*/camtasia\.dmg)(?:&quot;&gt;)</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>URLDownloader</string>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>%match%</string>
				<key>filename</key>
				<string>%NAME%.dmg</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
